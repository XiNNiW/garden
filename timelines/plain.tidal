let chords = slow 8 ( cat ["{ f'maj9 d'min7 }%4", "{cs'maj7 as'min11}%2"] ::Pattern Note)
    line1 = "<<0 -2> 1>" + "<0 -1>(<3 5>, 16, <5 7 9 13>)"
    kick = "0(<2 3 <5 1> <4 3>>,8)"
in
d1
$ stack [
   note(fingerpick chords $ kick) #s "midi" #midichan 0 #octave 3
   ,
   note "~ 0" #s "midi" #midichan 1
   ,
   note (fingerpick chords $ "[0 1 ~ <2 [-1 0 1]>]" + kick) #s "midi" #midichan 3 #octave 3
   ,
   every 2 ( sometimes (rev . stut 3 0.6 0.125)) $ note "0(<6 5>,16,1 5 1 9)" #s "midi" #midichan 2
   ,
   note (fingerpick chords $ iter 8 $ slow 2 $ run 4) #s "midi" #midichan 4
   ,
   note (fingerpick chords $ "[0,4,<5 [2 3]>]") #s "midi" #midichan 5
] #bpm 133

